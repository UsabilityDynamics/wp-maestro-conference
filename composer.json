{
  "name": "usabilitydynamics/wp-maestro-conference",
  "type": "wordpress-plugin",
  "minimum-stability": "dev",
  "homepage": "https://usabilitydynamics.com",
  "author": {
    "name": "UsabilityDynamics, Inc.",
    "url": "https://usabilitydynamics.com"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/usabilitydynamics/wp-maestro-conference.git"
  },
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-model": "0.4.0",
    "usabilitydynamics/lib-settings": "0.2.3",
    "usabilitydynamics/lib-wp-bootstrap": "*",
    "usabilitydynamics/lib-wp-shortcode": "0.1.0",
    "usabilitydynamics/lib-ui": "0.2.1",
    "usabilitydynamics/lib-maestro-conference-php-client": "*"
  },
  "require-dev": {
    "usabilitydynamics/lib-wp-phpunit": "~0.2",
    "codeclimate/php-test-reporter": "~0.1"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "archive": {
    "exclude": [
      "vendor",
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  },
  "extra": {
    "schemas": {
      "dependencies" : {
        "modules": {},
        "plugins": [
          {
            "name": "Meta Box",
            "slug": "meta-box",
            "required": true
          }
        ]
      },
      "model": {
        "types": {
          "maestro_conference": {
            "key": "maestro_conference",
            "data": {
              "capability_type": "page",
              "label": "Conferences",
              "labels": {
                "add_new": "New Conference",
                "add_new_item": "Create New Conference",
                "all_items": "All Conferences",
                "edit_item": "Edit Conference",
                "menu_name": "Conferences",
                "name": "Conferences",
                "not_found": "No Conferences found.",
                "not_found_in_trash": "No Conferences in Trash",
                "search_items": "Search Conference",
                "singular_name": "Conferences",
                "update_item": "Update Conference",
                "view_item": "View Conference"
              },
              "description": "Maestro Conferences",
              "exclude_from_search": false,
              "has_archive": true,
              "hierarchical": false,
              "menu_icon": false,
              "menu_position": 3,
              "public": true,
              "publicly_queryable": true,
              "show_in_admin_bar": true,
              "show_in_menu": true,
              "show_in_nav_menus": true,
              "show_ui": true,
              "supports": [
                "title",
                "editor",
                "excerpt"
              ],
              "can_export": true
            },
            "taxonomies": [
              "conference_tags"
            ],
            "meta": {
              "admin_information": {
                  "fields": [
                      "mc_status",
                      "mc_scheduledStartDate",
                      "mc_isPaid"
                  ]
              }
            }
          }
        },
        "meta": {
          "mc_status": {
            "name": "Status",
            "desc": "Status of conference",
            "type": "select",
            "options": {
              "active": "Active",
              "closed": "Closed"
            }
          },
          "mc_isPaid": {
            "name": "Paid",
            "desc": "Is paid conference",
            "type": "select",
            "options": {
              "paid": "Paid",
              "free": "Free"
            }
          },
          "mc_scheduledStartDate": {
            "name": "Start Date",
            "desc": "Scheduled starting date of meeting",
            "type": "datetime",
            "js_options": [
                {
                "minDate": "0"
                }
            ]
          }
        }
      },
      "settings": {
        "api": {
          "customer": "",
          "auth_key": "",
          "conference_uid": ""
        },
        "conference": {
          "reg_is_enabled": "true",
          "reg_is_disabled_for_nonlogged_users": "true",
          "user_can_cancel_reg": "true",
          "cancel_reg_max_time": "24"
        }
      },
      "ui": {
        "configuration": {
          "secondary_menu": {
            "page_title": "l10n.maestro_conference_page_title",
            "menu_title": "l10n.maestro_conference_settings",
            "parent_slug": "edit.php?post_type=maestro_conference",
            "menu_slug": "maestro_conferences_settings"
          }
        },
        "menu": {
          "api": {
            "id": "api",
            "name": "l10n.mc_api",
            "desc": "l10n.mc_api_credentials"
          },
          "meta": {
            "id": "meta",
            "name": "l10n.mc_meta_fields",
            "desc": "l10n.mc_meta_fields_availability"
          },
          "conference": {
            "id": "conference",
            "name": "l10n.mc_conference",
            "desc": "l10n.mc_general_pt_setting"
          }
        },
        "sections": {
          "credentials": {
            "id": "credentials",
            "name": "l10n.mc_credentials",
            "menu": "api"
          },
          "registration": {
            "id": "registration",
            "name": "l10n.mc_registration",
            "menu": "conference"
          },
          "pre_registration": {
            "id": "pre_registration",
            "name": "l10n.mc_pre_registration",
            "menu": "conference"
          },
          "meta_fields": {
            "id": "meta_fields",
            "name": "l10n.mc_meta_fields_availability",
            "menu": "meta"
          }
        },
        "fields": [
          {
            "id": "api.customer",
            "name": "l10n.mc_customer_uid",
            "section": "credentials"
          },
          {
            "id": "api.auth_key",
            "name": "l10n.mc_security_token",
            "section": "credentials"
          },
          {
            "id": "api.conference_uid",
            "name": "l10n.mc_conference_unique_id",
            "section": "credentials"
          }
        ]
      }
    }
  }
}